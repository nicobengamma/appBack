<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mascotas</title>
</head>
<body>
    <h3><%=mensaje %></h3>

    <ul>
        <% mascotas.forEach(function(mascotas) { %>
            <li>
                <strong><%= mascotas.name %></strong>
                <i><%= mascotas.animal %></i>
                <%= mascotas.edad %>
            </li>
        <% }) %>
    </ul>
</body>
</html>
